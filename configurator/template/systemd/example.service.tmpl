[Unit]
Description=Roboapp {{name}} Service
After=network-online.target
Wants=network-online.target

[Service]
WorkingDirectory={{app_folder}}/{{working_dir}}
ExecStart={{app_folder}}/{{working_dir}}/{{executable}} {{args}}
ExecStop=/bin/kill ${MAINPID}
Restart=always
RestartSec=3

[Install]
WantedBy=default.target

